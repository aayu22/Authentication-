{
    "manifest_version": 2,
  
    "name": "EduChrome",
    "description": "An educational Chrome extension!",
    "version": "1.0",
    "key": "",
    "oauth2": {
      "client_id": "517583566377-cttno07a4tbqsh8niti80omk21i7rvh9.apps.googleusercontent.com",
      "scopes": [
        "https://www.googleapis.com/auth/userinfo.email",
        "https://www.googleapis.com/auth/userinfo.profile"
      ]
    },
    "content_security_policy":"script-src 'self' https://www.gstatic.com/ https://*.firebaseio.com https://www.googleapis.com; object-src 'self'",
    "permissions":[
      "http://127.0.0.1/*" ,"identity" 
    ],
    "icons": {
      "128": "icon128.png",
      "48": "icon48.png",
      "16": "books1.png"
    },

    "browser_action": {
      "default_icon": "books1.png",
      "default_popup": "credentials.html" 
    },
    "background": {
      "page": "background.html"
    },
    "web_accessible_resources": ["js/jquery.min.js", "js/bootstrap.min.js"]
}